<h3> "Edit Song Form" </h3>
<%= form_for(@song) do |f|%>
    <%= f.label 'Title:'%><br>
    <%= f.text_field :title %><br>
    <%= f.label :released %><br>
    <%= f.check_box :released %>
    <%= f.label 'Year:'%><br>
    <%= f.text_field :release_year%><br>
    <%= f.label 'Artist:'%><br>
    <%= f.text_field :artist_name %><br>
    <%= f.label 'Genre:'%><br>
    <%= f.text_field :genre%>
    <%= f.submit %>
<% end %>